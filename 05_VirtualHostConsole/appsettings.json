{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Yanets.VirtualHostConsole": "Debug"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "HH:mm:ss "
      }
    }
  },
  "Network": {
    "DefaultSubnet": "192.168.1.0/24",
    "Gateway": "192.168.1.1",
    "StartPort": 23000,
    "MaxPort": 24000,
    "AutoStart": false,
    "MaxHosts": 100,
    "SessionTimeoutMinutes": 30
  },
  "Hosts": {
    "Defaults": {
      "Vendor": "cisco",
      "DeviceType": "router",
      "AutoStart": false
    }
  },
  "Security": {
    "DefaultUsername": "admin",
    "DefaultPassword": "password",
    "SnmpCommunity": "public",
    "RequireAuthentication": true
  },
  "Performance": {
    "StatisticsUpdateIntervalSeconds": 5,
    "MaxConcurrentConnections": 1000,
    "ConnectionTimeoutSeconds": 30
  }
}